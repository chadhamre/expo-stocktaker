{
  "expo": {
    "name": "Stock Taker for Shopify",
    "slug": "stock-taker-shopify",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "primaryColor": "#2f95dc",
    "scheme": "myapp",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["assets/images/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.chadhamre.stocktaker",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.chadhamre.stocktaker"
      "versionCode": 1
    },
    "description": "Scan barcodes and update stock counts on Shopify.",
    "extra": {
      "SERVER_HOST": "https://server-stocktaker.herokuapp.com"
    }
  }
}
